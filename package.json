    "jwt-decode": "^2.2.0",
    "load-script": "^1.0.0",
    "loader-utils": "^3.2.1",
    "lodash": "4.17.21",
    "moment": "^2.29.4",
    "npm": "^10.9.0",
    "path": "0.12.7",
    "qrcode": "1.5.0",
    "quasar": "2.17.0",
    "stream-browserify": "^3.0.0",
    "terser": "^5.14.2",
    "tiny-emitter": "2.1.0",
